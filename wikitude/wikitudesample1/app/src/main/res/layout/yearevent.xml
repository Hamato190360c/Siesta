<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gashfara.wikitudesample1.YearEventActivity"
    android:background="@drawable/background_event">

<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <ImageButton

        android:layout_width="360dp"
        android:layout_height="80dp"
        android:layout_marginTop="70dp"
        android:layout_marginLeft="0dp"
        android:background="@drawable/yearevent_venner" />

    <ImageButton
        android:id="@+id/newevent"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="2dp"
        android:background="@drawable/newevent" />

    <ImageButton
        android:id="@+id/new_1"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/new_1" />

    <ImageButton
        android:id="@+id/new_2"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/new_2" />

    <ImageButton
        android:id="@+id/new_3"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/new_3" />

    <ImageButton
        android:id="@+id/january"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="2dp"
        android:background="@drawable/jan_venner" />

    <ImageButton
        android:id="@+id/jan_1"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/jan_1" />
    <ImageButton
        android:id="@+id/february"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="2dp"
        android:background="@drawable/feb_venner" />



    <ImageButton
        android:id="@+id/feb_1"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/feb_1" />

    <ImageButton
        android:id="@+id/feb_2"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/feb_2" />
    <ImageButton
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="2dp"
        android:background="@drawable/mar_venner" />

    <ImageButton

        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/mar_1" />


</LinearLayout>

</ScrollView>