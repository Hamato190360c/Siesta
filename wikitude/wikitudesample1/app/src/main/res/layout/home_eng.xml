<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/background1"
    tools:context="com.gashfara.wikitudesample1.HomeEngActivity">

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="300dp"
        android:layout_height="150dp"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/ar_button" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="140dp"
        android:layout_height="150dp"
        android:layout_marginTop="180dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/mapeng_button" />

    <ImageButton
        android:id="@+id/animalengButton"
        android:layout_width="140dp"
        android:layout_height="139dp"
        android:layout_marginTop="180dp"
        android:layout_marginLeft="190dp"
        android:background="@drawable/animaleng_button" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="168dp"
        android:layout_height="150dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="330dp"
        android:background="@drawable/eventeng_button" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="160dp"
        android:layout_height="140dp"
        android:layout_marginTop="330dp"
        android:layout_marginLeft="180dp"
        android:background="@drawable/congrationeng_button" />

    <ImageButton
        android:id="@+id/button6"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginTop="480dp"
        android:layout_marginLeft="90dp"
        android:background="@drawable/language_button" />




</RelativeLayout>